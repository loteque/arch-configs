{
    "git.openRepositoryInParentFolders": "always",
    "git.autofetch": true,
    "godot_tools.gdscript_lsp_server_port": 6005,
    "godotTools.lsp.serverPort": 6005,
    "workbench.colorTheme": "737ClassicPlumNoMilk",
    "zenMode.fullScreen": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.showTabs": "none",
    "window.customTitleBarVisibility": "auto",
    "window.experimentalControlOverlay": false,
    "window.menuBarVisibility": "toggle",
    "window.titleBarStyle": "custom",
    "window.restoreWindows": "preserve",
    "window.zoomLevel": 0.7,
    "window.newWindowDimensions": "inherit",
    "window.openFoldersInNewWindow": "off",
    "window.openWithoutArgumentsInNewWindow": "off",
    "breadcrumbs.icons": false,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "diffEditor.renderSideBySide": false,
    "terminal.integrated.fontFamily": "'Symbols Nerd Font mono', 'NovaMono', 'monospace'",
    "terminal.integrated.defaultLocation": "editor",
    "terminal.integrated.hideOnStartup": "always",
    "chat.commandCenter.enabled": false,
    "excalidraw.theme": "auto",
    "editor.fontFamily": "'Hack', 'monospace', monospace",
    "editor.bracketPairColorization.enabled": false,
    "editor.guides.bracketPairs": "active",
    "editor.scrollbar.horizontalScrollbarSize": 14,
    "editor.scrollbar.verticalScrollbarSize": 16,
    "editor.tabCompletion": "on",
    "editor.minimap.enabled": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "workbench.layoutControl.enabled": false,
    "workbench.navigationControl.enabled": false,
    "workbench.startupEditor": "terminal",
    "workbench.editor.editorActionsLocation": "titleBar",
    "workbench.editor.showIcons": false,
    "workbench.editor.showTabs": "none",
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.folders.theme": "none",
    "material-icon-theme.saturation": 0.45,
    "material-icon-theme.hidesExplorerArrows": true,

}